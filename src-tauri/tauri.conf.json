{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "statuspageio-app",
    "version": "0.0.4"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/RemcoDewlde/StatusPage/releases/download/app-v{{current_version}}/statuspageio-app_{{current_version}}_{{arch}}"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEQxMkIzMzY4NENEMUQyMjUKUldRbDB0Rk1hRE1yMFNKcjBxdlVWNFZRc2lBOGRhaFNtQnhXN1llOE85UzJ4SDU2c3l4TVZQU1MK",
      "windows": {
        "installMode": "passive"
      }
    },
    "windows": [
      {
        "title": "statuspageio-app",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "party.littledonut.status",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}